            <taxonset idref="TaxonSet.h3n2_2deme"/>
        </tree>
        <parameter id="clockRate.c:h3n2_2deme" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
        <parameter id="BDBirthRate.t:h3n2_2deme" spec="parameter.RealParameter" lower="0.0" name="stateNode" upper="10000.0">1.0</parameter>
        <parameter id="BDDeathRate.t:h3n2_2deme" spec="parameter.RealParameter" lower="0.0" name="stateNode" upper="1.0">0.5</parameter>
    </state>

    <init id="RandomTree.t:h3n2_2deme" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:h3n2_2deme" taxa="@h3n2_2deme" adjustTreeNodeHeights="false">
        <populationModel id="ConstantPopulation0.t:h3n2_2deme" spec="ConstantPopulation">
            <parameter id="randomPopSize.t:h3n2_2deme" spec="parameter.RealParameter" name="popSize">1.0</parameter>
        </populationModel>
    </init>

    <distribution id="posterior" spec="util.CompoundDistribution">
