            <distribution id="BirthDeath.t:h3n2_2deme" spec="beast.evolution.speciation.BirthDeathGernhard08Model" birthDiffRate="@BDBirthRate.t:h3n2_2deme" relativeDeathRate="@BDDeathRate.t:h3n2_2deme" tree="@Tree.t:h3n2_2deme"/>
            <prior id="BirthRatePrior.t:h3n2_2deme" name="distribution" x="@BDBirthRate.t:h3n2_2deme">
                <Uniform id="Uniform.3" name="distr" upper="1000.0"/>
            </prior>
            <prior id="DeathRatePrior.t:h3n2_2deme" name="distribution" x="@BDDeathRate.t:h3n2_2deme">
                <Uniform id="Uniform.4" name="distr"/>
            </prior>
            <prior id="ClockPrior.c:h3n2_2deme" name="distribution" x="@clockRate.c:h3n2_2deme">
                <Uniform id="Uniform.0" name="distr" upper="Infinity"/>
            </prior>
        </distribution>
