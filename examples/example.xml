		    	  spec="organoidTreeLikelihood" scarringTime="2003.0" tree="@Tree.t:h3n2_2deme"
			      data="@h3n2_2deme">

        </distribution-->
        <distribution id="likelihood" spec="util.CompoundDistribution">
            <distribution id="treeLikelihood.h3n2_2deme" spec="organoidTreeLikelihood" data="@h3n2_2deme" scarringTime="1.0" tree="@Tree.t:h3n2_2deme">

                <siteModel id="SiteModel.s:h3n2_2deme" spec="SiteModel">
                    <parameter id="mutationRate.s:h3n2_2deme" spec="parameter.RealParameter" estimate="false" name="mutationRate">1.0</parameter>
                    <parameter id="gammaShape.s:h3n2_2deme" spec="parameter.RealParameter" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:h3n2_2deme" spec="parameter.RealParameter" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="JC69.s:h3n2_2deme" spec="JukesCantor"/>
                </siteModel>
                <branchRateModel id="StrictClock.c:h3n2_2deme" spec="beast.evolution.branchratemodel.StrictClockModel" clock.rate="@clockRate.c:h3n2_2deme"/>
            </distribution>
        </distribution>
    </distribution>

    <operator id="StrictClockRateScaler.c:h3n2_2deme" spec="ScaleOperator" parameter="@clockRate.c:h3n2_2deme" scaleFactor="0.75" weight="3.0"/>

    <operator id="strictClockUpDownOperator.c:h3n2_2deme" spec="UpDownOperator" scaleFactor="0.75" weight="3.0">
        <up idref="clockRate.c:h3n2_2deme"/>
        <down idref="Tree.t:h3n2_2deme"/>
    </operator>

    <operator id="BirthDeathTreeScaler.t:h3n2_2deme" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:h3n2_2deme" weight="3.0"/>

    <operator id="BirthDeathTreeRootScaler.t:h3n2_2deme" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:h3n2_2deme" weight="3.0"/>

    <operator id="BirthDeathUniformOperator.t:h3n2_2deme" spec="Uniform" tree="@Tree.t:h3n2_2deme" weight="30.0"/>

    <operator id="BirthDeathSubtreeSlide.t:h3n2_2deme" spec="SubtreeSlide" tree="@Tree.t:h3n2_2deme" weight="15.0"/>

    <operator id="BirthDeathNarrow.t:h3n2_2deme" spec="Exchange" tree="@Tree.t:h3n2_2deme" weight="15.0"/>

    <operator id="BirthDeathWide.t:h3n2_2deme" spec="Exchange" isNarrow="false" tree="@Tree.t:h3n2_2deme" weight="3.0"/>

    <operator id="BirthDeathWilsonBalding.t:h3n2_2deme" spec="WilsonBalding" tree="@Tree.t:h3n2_2deme" weight="3.0"/>

    <operator id="BirthRateScaler.t:h3n2_2deme" spec="ScaleOperator" parameter="@BDBirthRate.t:h3n2_2deme" scaleFactor="0.75" weight="3.0"/>

    <operator id="DeathRateScaler.t:h3n2_2deme" spec="ScaleOperator" parameter="@BDDeathRate.t:h3n2_2deme" scaleFactor="0.75" weight="3.0"/>

    <logger id="tracelog" spec="Logger" fileName="h3n2_2deme.log" logEvery="1000" model="@posterior" sanitiseHeaders="true" sort="smart">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log idref="treeLikelihood.h3n2_2deme"/>
        <log id="TreeHeight.t:h3n2_2deme" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.t:h3n2_2deme"/>
        <log idref="clockRate.c:h3n2_2deme"/>
        <log idref="BirthDeath.t:h3n2_2deme"/>
        <log idref="BDBirthRate.t:h3n2_2deme"/>
